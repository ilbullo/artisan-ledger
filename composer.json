{
    "name": "ilbullo/artisan-ledger",
    "description": "Modulo agnostico per la gestione dei corrispettivi giornalieri",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ilbullo\\ArtisanLedger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ilbullo\\ArtisanLedger\\Tests\\": "tests/"
        }
    },
    "extra": {
